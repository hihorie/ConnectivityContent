                  "yoy_total": {
                    "description": "Price for total - Delta",
                    "type": "number",
                    "format": "double"
                  },
                  "year_month": {
                    "description": "YYYYMM",
                    "type": "string",
                    "format": "yyyymm"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}